{
  "manifest_version": 3,
  "name": "VVTab - My New Tab",
  "version": "1.0.0",
  "description": "基于 Vue3 的自定义新标签页",
  "permissions": ["storage", "unlimitedStorage"],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "action": {
    "default_title": "VVTab"
  },
  "icons": {
    "16": "vite.svg",
    "48": "vite.svg",
    "128": "vite.svg"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self';"
  }
}
